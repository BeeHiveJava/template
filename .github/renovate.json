
{
  "suppressNotifications": ["prIgnoreNotification"],
  "extends": [
    "config:base",
    ":enablePreCommit",
    ":disableDependencyDashboard",
    ":disableRateLimiting",
    ":semanticCommits"
  ],
  "packageRules": [
    {
      "groupName": "npm dependencies",
      "groupSlug": "npm",
      "matchPackagePatterns": ["*"],
      "matchUpdateTypes": ["minor", "patch"]
    },
    {
      "groupName": "nuget dependencies",
      "groupSlug": "nuget",
      "matchPackagePatterns": ["*"],
      "matchUpdateTypes": ["minor", "patch"]
    }
  ]
}
